    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.4",
    "jest": "^29.3.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
